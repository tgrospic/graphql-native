{
  "name": "graphql-native",
  "version": "0.0.1",
  "private": true,
  "author": "Tomislav Grospic",
  "description": "GraphQL native",
  "main": "src/index.js",
  "scripts": {
    "nodemon": "nodemon src/index.js",
    "start": "nodemon --exec babel-node src/index.js --presets=@babel/preset-env",
    "build": "rimraf build; babel --presets=@babel/preset-env -d build --copy-files src",
    "upgrade": "npm-check --update"
  },
  "dependencies": {
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "log4js": "^4.0.2",
    "nodemon": "^1.18.10",
    "ramda": "^0.26.1",
    "sequelize": "^4.42.0",
    "sequelize-values": "^1.1.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "npm-check": "^5.9.0",
    "rimraf": "^2.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "db/*",
      "build/*"
    ]
  }
}
