{
  "name": "graphql-native",
  "version": "0.0.1",
  "private": true,
  "author": "Tomislav Grospic",
  "description": "GraphQL native",
  "main": "src/index.js",
  "scripts": {
    "nodemon": "nodemon src/index.js",
    "start": "nodemon --exec babel-node src/index.js --presets=@babel/preset-env",
    "build": "rimraf build; babel --presets=@babel/preset-env -d build --copy-files src",
    "upgrade": "npm-check --update"
  },
  "dependencies": {
    "express": "^4.17.0",
    "express-graphql": "^0.7.1",
    "graphql": "^14.3.0",
    "graphql-iso-date": "^3.6.1",
    "log4js": "^4.2.0",
    "nodemon": "^1.19.0",
    "ramda": "^0.26.1",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "npm-check": "^5.9.0",
    "rimraf": "^2.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "db/*",
      "build/*"
    ]
  }
}
